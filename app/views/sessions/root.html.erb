var multi = false;

$('#air_type').change(function(){
	if($(this).val() == 'multicity'){
		multi = true;
		$('.top-group').addClass('top-group-border');
		$('.bottom-group').addClass('bottom-group-border');
		$('.bottom-group').removeClass('remove-bottom-margin');
		$('.bottom-group').removeClass('hide');
