		$('.multi-button').addClass('button-show');
		$('.multi-button').removeClass('hide');
		$('.multiDiv').removeClass('hide');
		$('.multiDiv').addClass('block');
	}
	else if($(this).val() == 'oneway'){
		if(multi == true){
			$('.append-div').empty();
			multi = false;
		}
		$('.return-form-group').addClass('block');
		$('.return-form-group').removeClass('hide');
