	<link href='http://fonts.googleapis.com/css?family=Droid+Sans' rel='stylesheet'  type='text/css'>
	<link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
	<style type="text/css">
		*{
			font-family: 'Open Sans', sans-serif;
			color: black;
		}
		.nav-container{
		    background: white;
		    height: 100px;
		    padding-top: 20px;
		}
		.navbar-default{
			box-shadow: none;
    		border-bottom: none;
		}
		.navbar-nav li{
			/*margin: 0px 15px;*/
		}
		.navbar{
			background: white;
		}
		.navbar-default .navbar-nav>li>a {
		    color: black;
		    font-size: 1em;
		}
	    .dropdown-toggle:active, .open .dropdown-toggle {
	        background:#FFF !important; 
	        color:#000 !important;
	    }
	    .navbar-right{
	    	margin-top: 15px;
	    }
	    .navbar-brand img{
	    	height: 80px;
	    	display: inline-block;
	    	margin-top: -35px;
	    }
	    .navbar-brand{
	    	padding: 0px;
	    }
	    .project-name{
		    color: black;
		    font-size: 2em;
		    text-align: center;
		    display: block;
	    }
	    #corp{
	    	font-size: 1em;
	    	text-align: center;
	    	margin-top: 2px;
	    	color: #33525e;
	    }
	    .logo-text{
	    	display: inline-block;
	    	margin-top: 20px;
	    	margin-left: 40px;
	    }
.btn-sample { 
  color: #FFFFFF; 
  background-color: #33525E; 
  border-color: #0A1012; 
} 
 
.btn-sample:hover, 
.btn-sample:focus, 
.btn-sample:active, 
.btn-sample.active, 
.open .dropdown-toggle.btn-sample { 
  color: #FFFFFF; 
  background-color: #1C2C33; 
  border-color: #0A1012; 
} 
 
.btn-sample:active, 
.btn-sample.active, 
.open .dropdown-toggle.btn-sample { 
  background-image: none; 
} 
 
.btn-sample.disabled, 
.btn-sample[disabled], 
fieldset[disabled] .btn-sample, 
.btn-sample.disabled:hover, 
.btn-sample[disabled]:hover, 
fieldset[disabled] .btn-sample:hover, 
.btn-sample.disabled:focus, 
.btn-sample[disabled]:focus, 
fieldset[disabled] .btn-sample:focus, 
.btn-sample.disabled:active, 
.btn-sample[disabled]:active, 
fieldset[disabled] .btn-sample:active, 
.btn-sample.disabled.active, 
.btn-sample[disabled].active, 
fieldset[disabled] .btn-sample.active { 
  background-color: #33525E; 
  border-color: #0A1012; 
} 
 
.btn-sample .badge { 
  color: #33525E; 
  background-color: #FFFFFF; 
}
	</style>
  </head>

  <body>
    <!-- Static navbar -->
    <nav class="navbar navbar-default navbar-static-top">
      <div class="container nav-container">
